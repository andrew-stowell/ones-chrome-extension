{
  "name": "Ones Better Styles",
  "description": "Enhanced styles for Ones",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://ones.com/*"],
      "css": ["ones-enhanced-styles.css"]
    }
  ]
}
